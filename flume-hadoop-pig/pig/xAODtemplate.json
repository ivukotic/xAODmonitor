{
  "template" : "xAOD-data-20*",
  "settings" : {
    "analysis" : {
      "analyzer": {
        "lowercase_whitespace": {
          "type": "custom",
          "tokenizer":  "whitespace",
          "filter": ["lowercase"]
        }
      }
    },
    "index" : {
        "number_of_replicas" : 3
    }
  },
  "mappings" : {
    "_default_": {
      "properties": {
        "F": {
          "type" : "long",
          "doc_values": true
        },
        "S": {
          "type": "long",
          "doc_values": true
        },
        "T": {
          "type" : "long",
          "doc_values": true
        },
        "CLOUD": {
          "type": "string",
          "analyzer": "lowercase_whitespace"
        }
      }
    }
  }
}